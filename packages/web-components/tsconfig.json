{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "types": ["node", "@stencil/core"],
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}

